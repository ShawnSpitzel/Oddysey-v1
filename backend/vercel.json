{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      },
      {
        "src": "/start_conversation",
        "dest": "main.py"
      },
      {
        "src": "/receive_response",
        "dest": "main.py"
      }
    ]
  }
  